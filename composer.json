{
    "name": "kchinkesh/laravel-model-observer",
    "description": "A Simple Laravel Package to observe,log and anlaize any changes to Eloquent Model using trait.",
    "license": "MIT",
	"require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0"
    },
	"require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "authors": [
        {
            "name": "Chinkesh Kumar",
            "email": "kumar.chinkesh@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Kchinkesh\\Laravel-model-observer\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kchinkesh\\LaravelModelObserver\\Providers\\ModelObserverServiceProvider"
            ]
        }
    }
}

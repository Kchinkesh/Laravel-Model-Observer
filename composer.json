{
    "name": "kchinkesh/laravel-model-observer",
    "description": "A Simple Laravel Package to observe,log and anlaize any changes to Eloquent Model using trait.",
    "license": "MIT",
    "authors": [
        {
            "name": "Chinkesh Kumar",
            "email": "kumar.chinkesh@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3.0|^8.0",
        "laravel/ui": "^3.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "kchinkesh\\LaravelModelObserver\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "kchinkesh\\LaravelModelObserver\\ModelObserverServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
